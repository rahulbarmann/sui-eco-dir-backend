# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration
# Add all allowed origins separated by commas
CORS_ORIGINS=http://localhost:5173,http://localhost:5174,https://your-frontend-domain.up.railway.app,https://your-admin-frontend-domain.up.railway.app

# API Configuration
API_PREFIX=/api/v1

# Logging
LOG_LEVEL=info

# Auth
JWT_SECRET=change_me_in_production
JWT_EXPIRES_IN=7d

# Database Configuration
DATABASE_URL="postgresql://postgres:password@localhost:5432/sui_eco_dir?schema=public"

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here
AWS_REGION=us-east-1
AWS_S3_BUCKET_NAME=your_s3_bucket_name_here

# CDN Configuration
CDN_BASE_URL=https://xyz.cloudfront.net